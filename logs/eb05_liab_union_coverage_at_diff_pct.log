. //==============================================================================
. //1. Union coverage at different percentile categories
. //==============================================================================
. 
. /*
> Dieses Do-File gibt Summary Statistics von Tarifvertragsdummies fuer 4 Perzentilkategorien aus 
> */
. 
. 
. //Load the LIAB data
. use "${data}/liab_panel.dta", clear
(FDZ1490 LIAB Panel -  6 Oct 2022 - 12:20:42)

.  
. //generate help variables for counter
. bys east year persnr: gen N_Pers =(_n==1)

. bys east year idnum:  gen N_Betr =(_n==1)

. 
. 
. //Generate wage percentiles
. sort east year

. foreach p in "15" "50" "85"{
  2.         by east year: egen mwage_p`p' = pctile(mwage), p(`p')
  3. }

. 
. gen pct_class = .
(20,331,955 missing values generated)

. replace pct_class = 1 if mwage <= mwage_p15
(3,050,045 real changes made)

. replace pct_class = 2 if mwage > mwage_p15 & mwage <= mwage_p50
(7,116,138 real changes made)

. replace pct_class = 3 if mwage > mwage_p50 & mwage <= mwage_p85
(7,224,640 real changes made)

. replace pct_class = 4 if mwage > mwage_p85
(2,941,132 real changes made)

. #delimit ;
delimiter now ;
. label define pct_class_lab 1 "Sub_15"
>                                                    2 "15_to_50"
>                                                    3 "50_to_85"
>                                                    4 "over85";

. #delimit cr
delimiter now cr
. label values pct_class pct_class_lab

. 
. //keep only the needed years
. keep if year==1997 | year==2004 | year==2010
(17,117,752 observations deleted)

. 
. //Generate Tarifudmmies
. gen t_branche = tarif==1

. gen t_haus  = tarif==2

. 
. //Drop if east is missing
. drop if east==.
(/ observations deleted)

. 
. 
. /*Hier werden jeweils der Mittelwert, die Standardabweichung, das Minimum und Maximum 
> und die verwendete Anzahl fuer Personen fuer die Variablen t_branche (Dummy fuer Branchentarifvertraege)
> und t_haus (Dummy fuer Haustarifvertraege) ausgegeben. In einer Schleife werden einmal Werte ohne 
> Gewichtung und einmal mit Querschnittshochrechnung ausgegeben.
> 
> Es wird immer die Variable und die Art der Ausgabe angegeben. Die Werte werden nach West fÃ¼r die Jahre 1996 und 2005 
> und nach 4 Lohn-Perzentilklassen (unter 15, 15 bis 50, 50 bis 85, ueber 85) aufgeteilt ausgegeben. */
. 
. //Ausgabe-Schleife
. label define w_lab 1 "" 2 "[iweight=hrf_quer]" 

. label define w_readable 1 "ohne Gewichtung" 2 "mit Querschnttshochrechnung" 

. label define wmat 1 "raw" 2 "whrf" 

. foreach var in "branche" "haus"{
  2.         display "=================================================="
  3.         display "Variable: `var'"
  4.         forvalues w = 1/2{
  5.                 
.                 local weight: label w_lab `w'
  6.                 local title: label w_readable `w'
  7.                 local mw: label wmat `w'
  8.                 display "--------------------------------------------"
  9.                 display "`w'. Ausgabe  `title'"
 10.                 preserve 
 11.                 #delimit ;
delimiter now ;
.                         collapse (mean) m_`var'=t_`var' 
>                                         (sd) sd_`var'=t_`var' 
>                                         (min) min_`var'=t_`var'  
>                                         (max) max_`var'=t_`var'                                  
>                                         (sum) N_Pers 
>                                         (sum) N_Betr `weight', by(east year pct_class);
 12.                 #delimit cr
delimiter now cr
.                 mkmat east year pct_class m_`var' sd_`var' min_`var' max_`var' N_Pers N_Betr, mat(m_`var'_`mw')
 13.                 display ". matrix list m_`var'_`mw'"
 14.                 matrix list m_`var'_`mw'
 15.                 restore
 16.         }
 17. }
